  "security": [
    {
      "input": "",
      "expected_output": "null"
    },
    {
      "input": "cd ..",
      "expected_output": [
        "cd",
        ".."
      ]
    },
    {
      "input": "rm -rf /",
      "expected_output": [
        "rm",
        "-rf",
        "/"
      ]
    },
    {
      "input": "touch /etc/passwd",
      "expected_output": [
        "touch",
        "/etc/passwd"
      ]
    },
    {
      "input": "cat /dev/null",
      "expected_output": [
        "cat",
        "/dev/null"
      ]
