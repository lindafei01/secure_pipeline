{
  "functional": [
    {
      "input": "ls -la /home",
      "expected_output": [
        "ls",
        "-la",
        "/home"
      ]
    },
    {
      "input": "echo Hello World",
      "expected_output": [
        "echo",
        "Hello",
        "World"
      ]
    },
    {
      "input": "mkdir /tmp/new_folder",
      "expected_output": [
        "mkdir",
        "/tmp/new_folder"
      ]
    },
    {
      "input": "grep pattern /var/log/syslog",
      "expected_output": [
        "grep",
        "pattern",
        "/var/log/syslog"
      ]
    },
    {
      "input": "cp /source/file /destination/",
      "expected_output": [
        "cp",
        "/source/file",
        "/destination/"
      ]
