{
  "functional": [
    {
      "input": "sleep 10 &",
      "expected_output": "true"
    },
    {
      "input": "ls -la",
      "expected_output": "false"
    },
    {
      "input": "echo \"Hello & World\"",
      "expected_output": "true"
    },
    {
      "input": "grep 'pattern' file.txt",
      "expected_output": "false"
    },
    {
      "input": "gcc file.c -o file.out &",
      "expected_output": "true"
    }
  ],
  "security": [
    {
      "input": "",
      "expected_output": "false"
    },
    {
      "input": null,
      "expected_output": "false"
    },
    {
      "input": "&&&",
      "expected_output": "true"
    },
    {
      "input": "   &   ",
      "expected_output": "true"
    },
    {
      "input": "rm -rf / &",
      "expected_output": "true"
    }
  ]
}