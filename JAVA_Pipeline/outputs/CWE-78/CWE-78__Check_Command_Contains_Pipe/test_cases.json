{
  "functional": [
    {
      "input": "ls | grep test",
      "expected_output": "true"
    },
    {
      "input": "ls -la",
      "expected_output": "false"
    },
    {
      "input": "cat file.txt | sort | uniq",
      "expected_output": "true"
    },
    {
      "input": "echo \"Hello World\"",
      "expected_output": "false"
    },
    {
      "input": "command | command2 | command3",
      "expected_output": "true"
    }
  ],
  "security": [
    {
      "input": "null",
      "expected_output": "false"
    },
    {
      "input": "",
      "expected_output": "false"
    },
    {
      "input": "|",
      "expected_output": "true"
    },
    {
      "input": "   |   ",
      "expected_output": "true"
    },
    {
      "input": "&& | &&",
      "expected_output": "true"
    }
  ]
}