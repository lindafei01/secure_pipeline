{
  "functional": [
    {
      "input": "ls; rm file",
      "expected_output": "true"
    },
    {
      "input": "ls -la",
      "expected_output": "false"
    },
    {
      "input": "echo 'Hello, World!'; echo 'Goodbye!'",
      "expected_output": "true"
    },
    {
      "input": "mkdir new_folder",
      "expected_output": "false"
    },
    {
      "input": "date; uptime",
      "expected_output": "true"
    }
  ],
  "security": [
    {
      "input": "null",
      "expected_output": "false"
    },
    {
      "input": "",
      "expected_output": "false"
    },
    {
      "input": ";;",
      "expected_output": "true"
    },
    {
      "input": ";",
      "expected_output": "true"
    },
    {
      "input": "   ",
      "expected_output": "false"
    }
  ]
}