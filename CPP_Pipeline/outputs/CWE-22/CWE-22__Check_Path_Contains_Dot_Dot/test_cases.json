{
  "functional": [
    {
      "input": "../etc/passwd",
      "expected_output": "true"
    },
    {
      "input": "/home/user/file.txt",
      "expected_output": "false"
    },
    {
      "input": "/var/log/../logfile",
      "expected_output": "true"
    },
    {
      "input": "/usr/local/bin",
      "expected_output": "false"
    },
    {
      "input": "../../root/config.yaml",
      "expected_output": "true"
    }
  ],
  "security": [
    {
      "input": ".",
      "expected_output": "false"
    },
    {
      "input": "..",
      "expected_output": "true"
    },
    {
      "input": "/path/with/multiple/..//..//segments/",
      "expected_output": "true"
    },
    {
      "input": "",
      "expected_output": "false"
    },
    {
      "input": "/./././././",
      "expected_output": "false"
    }
  ]
}