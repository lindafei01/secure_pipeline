{
  "functional": [
    {
      "input": "/etc/passwd",
      "expected_output": "true"
    },
    {
      "input": "/home/user/file.txt",
      "expected_output": "false"
    },
    {
      "input": "/proc/cpuinfo",
      "expected_output": "true"
    },
    {
      "input": "/sys/class/net",
      "expected_output": "true"
    },
    {
      "input": "/var/log/syslog",
      "expected_output": "false"
    }
  ],
  "security": [
    {
      "input": "",
      "expected_output": "false"
    },
    {
      "input": "/etc/passwd/backup",
      "expected_output": "true"
    },
    {
      "input": "/etc/shadow",
      "expected_output": "true"
    },
    {
      "input": "../etc/passwd",
      "expected_output": "false"
    },
    {
      "input": "/sys/",
      "expected_output": "true"
    }
  ]
}