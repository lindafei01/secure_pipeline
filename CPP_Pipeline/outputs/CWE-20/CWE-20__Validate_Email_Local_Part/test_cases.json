{
  "functional": [
    {
      "input": "username123",
      "expected_output": "true"
    },
    {
      "input": "user.name_456",
      "expected_output": "true"
    },
    {
      "input": "user.name@domain.com",
      "expected_output": "false"
    },
    {
      "input": "user!name",
      "expected_output": "false"
    },
