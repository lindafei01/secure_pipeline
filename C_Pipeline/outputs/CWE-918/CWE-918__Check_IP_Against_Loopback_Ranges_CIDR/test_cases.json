{
  "functional": [
    {
      "input": "127.0.0.1",
      "expected_output": "1"
    },
    {
      "input": "127.255.255.255",
      "expected_output": "1"
    },
    {
      "input": "8.8.8.8",
      "expected_output": "0"
    },
    {
      "input": "192.168.1.1",
      "expected_output": "0"
    },
    {
      "input": "10.0.0.1",
      "expected_output": "0"
    }
  ],
  "security": [
    {
      "input": "127.0.0.1; rm -rf /",
      "expected_output": "1"
    },
    {
      "input": "8.8.8.8; shutdown -h now",
      "expected_output": "0"
    },
    {
      "input": "127.0.0.1' OR '1'='1",
      "expected_output": "1"
    },
    {
      "input": "127.0.0.1/*",
      "expected_output": "1"
    },
    {
      "input": "10.0.0.1'; DROP TABLE users;--",
      "expected_output": "0"
    }
  ]
}