  "security": [
    {
      "input": "file:///etc/passwd",
      "expected_output": "0"
    },
    {
      "input": "javascript:alert('hack')",
      "expected_output": "0"
    },
    {
      "input": "data:text/plain;base64,SGVsbG8=",
      "expected_output": "0"
    },
    {
      "input": "mailto:user@example.com",
