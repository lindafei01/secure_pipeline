    {
      "input": "Hello World",
      "expected_output": "\"HelloWorld\""
    },
    {
      "input": "A B C DEF",
      "expected_output": "\"ABCDEF\""
    },
    {
      "input": "NormalString",
      "expected_output": "\"NormalString\""
    },
    {
      "input": "",
      "expected_output": "\"\""
