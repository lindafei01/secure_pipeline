  "security": [
    {
      "input": "< sc r ipt >",
      "expected_output": "\"<script>\""
    },
    {
      "input": "java\nscript:",
      "expected_output": "\"javascript:\""
    },
    {
      "input": "alert(1); // \n comment",
      "expected_output": "\"alert(1);//comment\""
    },
    {
      "input": "< style >body{background-color:red;}</style >",
      "expected_output": "\"<style>body{background-color:red;}</style>\""
